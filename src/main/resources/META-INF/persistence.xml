		<class>com.sap.cloud.samples.ariba.partner.flow.persistency.entities.AttachmentDocument</class>
		<class>com.sap.cloud.samples.ariba.partner.flow.persistency.entities.ShipmentIdentifier</class>
		<class>com.sap.cloud.samples.ariba.partner.flow.persistency.entities.Event</class>
		<properties>
			<property name="eclipselink.ddl-generation" value="create-or-extend-tables" />
